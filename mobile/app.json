{
  "expo": {
    "name": "Tap Story",
    "slug": "tap-story",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.tapstory.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record audio stories.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.tapstory.app",
      "permissions": [
        "RECORD_AUDIO",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "image": "./assets/splash.png"
        }
      ],
      "./plugins/withTrackPlayer.js"
    ],
    "scheme": "tapstory",
    "extra": {
      "eas": {
        "projectId": "f1c2d6c7-5fe2-43fa-898d-aaf00bb8bbe8"
      }
    },
    "owner": "shantamg"
  }
}
