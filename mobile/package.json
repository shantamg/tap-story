{
  "name": "@tapstory/mobile",
  "main": "expo-router/entry",
  "version": "0.1.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "set-local-ip": "node ./scripts/set-local-ip.js",
    "start": "npm run set-local-ip && npm run start:expo",
    "start:expo": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "check": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(expo-modules-core|expo-asset|expo-router|@react-native|react-native|@expo|expo|@unimodules|unimodules|native-base|react-navigation|@react-navigation|@react-native-async-storage|react-native-gesture-handler|react-native-reanimated|react-native-screens|react-native-safe-area-context|expo-file-system|expo-constants|expo-linking|expo-font|expo-splash-screen|expo-status-bar|expo-system-ui|@expo/vector-icons)/)"
    ]
  },
  "dependencies": {
    "@expo/config-plugins": "~54.0.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native/metro-config": "^0.79.0",
    "@react-native/virtualized-lists": "0.79.5",
    "expo": "^54.0.0",
    "expo-av": "~16.0.7",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-font": "~14.0.9",
    "expo-linking": "~8.0.9",
    "expo-modules-core": "~3.0.26",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.13",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "sdk": "54"
  }
}
